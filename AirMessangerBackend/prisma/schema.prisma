generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  uniq_id  String @unique
  email     String?
  name      String?
  name_profile   String?
  last_name   String?
  phone_number   String?
  avatar String?
  createdAt DateTime @default(now())
}
model Chat {
  id_chat       Int      @id @default(autoincrement())
  id_user       Int?
  content   String?
  username  String?
  socketId  String?
  createdAt DateTime @default(now())
}
